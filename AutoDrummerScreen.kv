ScreenManagement:
    MainMenuScreen:
    DrumsScreen:

<MainMenuScreen>
    name: "MainMenuScreen"

    tempo_set: set_tempo


    GridLayout:
        size: root.width, root.height
        cols: 1

        GridLayout:                                 
            size: root.width, root.height
            size_hint: 0.1, 0.1
            cols: 6

            TextInput:
                id: set_tempo
                text: "80"

            Button:
                id: set_tempo_button
                text: "SET TEMPO"
                on_release: root.tempo()

            Label:
                text: "METRUM"

            Spinner:
                id: choose_metrum
                text: "CHOOSE METRUM"

                values: ["4", "8", "16", "32"]
                on_text: root.metrum(self.text)

        GridLayout:                                 
            size: root.width, root.height
            size_hint: 0.1, 0.1
            cols: 2

            Button:
                id: "generate"
                text: "GENERATE MP3"
                on_release: root.generate()

            Button:
                id: "play"
                text: "PLAY"
                on_release: root.play_sound("drums")


        GridLayout:
            size: root.width, root.height
            size_hint: root.width, 0.1
            cols: 1

            Button:
                text: "START COMPOSING"
                on_release:
                    app.root.current = "DrumsScreen"
                    root.manager.transition.direction = "left"


<DrumsScreen>
    name: "DrumsScreen"

    GridLayout:
        size: root.width, root.height
        cols: 1

        GridLayout:
            size: root.width, root.height
            cols: 2

            GridLayout:
                cols: 1
                size: root.width, root.height
                size_hint: 0.1, root.height

                Button:
                    text: "CRASH"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("crash")

                Button:
                    text: "RIDE"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("ride")

                Button:
                    text: "OPEN HAT"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("opened_hat")


                Button:
                    text: "CLOSE HAT"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("closed_hat")

                Button:
                    text: "HIGH TOM"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("high_tom")

                Button:
                    text: "MIDDLE TOM"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("middle_tom")

                Button:
                    text: "LOW TOM"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("low_tom")

                Button:
                    text: "SNARE"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("snare")

                Button:
                    text: "KICK"
                    on_release: root.manager.get_screen("MainMenuScreen").play_sound("kick")


            GridLayout:
                size: root.width, root.height
                pos: 0, 0
